<body id="top">

<!-- header
================================================== -->
<header>
    <div class="row">

        <div class="top-bar">
            <a data-test="menu-toggle" class="menu-toggle" href="#"><span>Menu</span></a>

            <div class="logo">
                <a href="index.html">Resume</a>
            </div>

            <nav id="main-nav-wrap">
                <ul class="main-navigation">
                    <li class="current"><a data-test="intro-link" class="smoothscroll"  href="#intro" title="">Home</a></li>
                    <li><a data-test="about-link" class="smoothscroll"  href="#about" title="">About</a></li>
                    <li><a data-test="resume-link" class="smoothscroll"  href="#resume" title="">Resume</a></li>
                    <li><a class="smoothscroll"  href="#portfolio" title="">Portfolio</a></li>
                    <li><a class="smoothscroll"  href="#services" title="">Services</a></li>
                    <li><a class="smoothscroll"  href="#contact" title="">Contact</a></li>
                </ul>
            </nav>
        </div>

    </div>
</header>

{{#resume.basics}}
    <!-- intro section
  ================================================== -->
    <section id="intro">
        <div class="intro-overlay"></div>
        <div class="intro-content">
            <div class="row">
                <div class="col-twelve">
                    <h5>print("Hello, World ðŸ‘‹")</h5>
                    <h1>I'm {{name}}.</h1>
                    <h5>{{label}}</h5>
                    <a class="button stroke smoothscroll" href="#about" title="">About Me</a>
                </div>
            </div>
        </div>

        <ul class="intro-social">
            {{#each profiles}}
                <li><a href="{{url}}"><i class="fa fa-{{toLowerCase network}}"></i></a></li>
            {{/each}}
        </ul>
    </section>
{{/resume.basics}}

<!-- about section
================================================== -->
<section id="about">
    {{#resume.basics}}
        <div class="row section-intro">
            <div class="col-twelve">
                <h5>About</h5>
                <h1>Let me introduce myself.</h1>
                <div class="intro-info">
                    {{#if image}}<img src="{{image}}" alt="Profile Picture">{{/if}}
                    <p class="lead">{{#markdown}}{{summary}}{{/markdown}}</p>
                </div>
            </div>
        </div>
    {{/resume.basics}}

    <div class="row about-content">
        <div class="col-twelve">
            <h3>Profile</h3>
            {{#resume.basics}}
                <ul class="info-list">
                    <li>
                        <strong>Fullname:</strong>
                        <span>{{name}}</span>
                    </li>
                    {{#if label}}
                        <li>
                            <strong>Job:</strong>
                            <span>{{label}}</span>
                        </li>
                    {{/if}}
                    {{#if url}}
                        <li>
                            <strong>Website:</strong>
                            <span>{{displayUrl url}}</span>
                        </li>
                    {{/if}}
                    {{#if email}}
                        <li>
                            <strong>Email:</strong>
                            <span>{{email}}</span>
                        </li>
                    {{/if}}
                </ul>
            {{/resume.basics}}
        </div>
    </div>

    <div class="row button-section">
        <div class="col-twelve">
            <a href="#contact" title="Hire Me" class="button stroke smoothscroll">Hire Me</a>
        </div>
    </div>
</section>

<!-- resume Section
================================================== -->
<section id="resume" class="grey-section">
    <div class="row section-intro">
        <div class="col-twelve">
            <h5>Resume</h5>
            <h1>More of my credentials.</h1>
        </div>
    </div>
    {{#if resume.work}}
        <div class="row resume-timeline">
            <div class="col-twelve resume-header">
                <h2>Work Experience</h2>
            </div>
            <div class="col-twelve">
                <div class="timeline-wrap">
                    {{#each resume.work}}
                        <div class="timeline-block">
                            <div class="timeline-ico">
                                <i class="fa fa-briefcase"></i>
                            </div>
                            <div class="timeline-header">
                                <h3>{{position}}</h3>
                                <p>{{date startDate}} - {{#if (endDatePresent endDate)}}Present{{else}}{{date endDate}}{{/if}}</p>
                            </div>
                            <div class="timeline-content">
                                <h4>{{name}}</h4>
                                <p>{{#markdown}}{{summary}}{{/markdown}}</p>
                            </div>
                        </div>
                    {{/each}}
                </div>
            </div>
        </div>
    {{/if}}
    {{#if resume.volunteer}}
        <div class="row resume-timeline">
            <div class="col-twelve resume-header">
                <h2>Volunteer Work</h2>
            </div>
            <div class="col-twelve">
                <div class="timeline-wrap">
                    {{#each resume.volunteer}}
                        <div class="timeline-block">
                            <div class="timeline-ico">
                                <i class="fa fa-handshake-o"></i>
                            </div>
                            <div class="timeline-header">
                                <h3>{{position}}</h3>
                                <p>{{date startDate}} - {{date endDate}}</p>
                            </div>
                            <div class="timeline-content">
                                <h4>{{organization}}</h4>
                                <p>{{#markdown}}{{summary}}{{/markdown}}</p>
                            </div>
                        </div>
                    {{/each}}
                </div>
            </div>
        </div>
    {{/if}}
    {{#if resume.publications}}
        <div class="row resume-timeline">
            <div class="col-twelve resume-header">
                <h2>Publications</h2>
            </div>
            <div class="col-twelve">
                <div class="timeline-wrap">
                    {{#each resume.publications}}
                        <div class="timeline-block">
                            <div class="timeline-ico">
                                <i class="fa fa-book"></i>
                            </div>
                            <div class="timeline-header">
                                <h3>{{name}}</h3>
                                <p>{{date releaseDate}}</p>
                            </div>
                            <div class="timeline-content">
                                <h4>{{publisher}}</h4>
                                <p>{{#markdown}}{{summary}}{{/markdown}}</p>
                            </div>
                        </div>
                    {{/each}}
                </div>
            </div>
        </div>
    {{/if}}
    {{#if resume.education}}
        <div class="row resume-timeline">
            <div class="col-twelve resume-header">
                <h2>Education</h2>
            </div>
            <div class="col-twelve">
                <div class="timeline-wrap">
                    {{#each resume.education}}
                        <div class="timeline-block">
                            <div class="timeline-ico">
                                <i class="fa fa-graduation-cap"></i>
                            </div>
                            <div class="timeline-header">
                                <h3>{{studyType}} Degree</h3>
                                <p>{{date startDate}} - {{date endDate}}</p>
                            </div>
                            <div class="timeline-content">
                                <h4>{{institution}}</h4>
                                <p>{{area}}</p>
                            </div>
                        </div>
                    {{/each}}
                </div>
            </div>
        </div>
    {{/if}}
</section>

<!-- portfolio Section
================================================== -->
<section id="portfolio">
    <div class="row section-intro">
